{
    "name": "laraning/boost",
    "description": "Utilities package that gives you an extra-boost in your Laravel applications development.",
    "type": "library",
    "keywords": [
        "utilities",
        "laraning",
        "laravel",
        "php"
    ],
    "require": {
        "illuminate/support": "^5.6",
        "php": "^7.1",
        "jacekbarecki/flysystem-onedrive": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "Laraning\\Boost\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laraning\\Boost\\Providers\\BaseServiceProvider",
                "Laraning\\Boost\\Providers\\OnedriveServiceProvider",
            ]
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Laraning",
            "email": "bruno.falcao@laraning.com"
        }
    ],
    "minimum-stability": "dev"
}
