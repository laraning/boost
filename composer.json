{
    "name": "waygou/boost",
    "description": "Utilities package that gives you an extra-boost in your Laravel applications development.",
    "type": "library",
    "keywords": [
        "utilities",
        "laraning",
        "laravel",
        "waygou",
        "php"
    ],
    "require": {
        "illuminate/support": "^5.6",
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "Waygou\\Boost\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Waygou\\Boost\\Providers\\BaseServiceProvider"
            ]
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Laraning",
            "email": "bruno.falcao@laraning.com"
        }
    ],
    "minimum-stability": "dev"
}
